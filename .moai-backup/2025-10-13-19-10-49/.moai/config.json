{
  "_meta": {
    "@CODE:CONFIG-STRUCTURE-001": "@DOC:JSON-CONFIG-001",
    "@SPEC:PROJECT-CONFIG-001": "@SPEC:MOAI-CONFIG-001"
  },
  "moai": {
    "version": "0.2.29"
  },
  "constitution": {
    "enforce_tdd": true,
    "principles": {
      "simplicity": {
        "max_projects": 5,
        "notes": "기본 권장값. 프로젝트 규모에 따라 .moai/config.json 또는 SPEC/ADR로 근거와 함께 조정하세요."
      }
    },
    "require_tags": true,
    "simplicity_threshold": 5,
    "test_coverage_target": 85
  },
  "git_strategy": {
    "personal": {
      "auto_checkpoint": true,
      "auto_commit": false,
      "branch_prefix": "feature/",
      "checkpoint_interval": 300,
      "cleanup_days": 7,
      "max_checkpoints": 50
    },
    "team": {
      "auto_pr": true,
      "develop_branch": "develop",
      "draft_pr": true,
      "feature_prefix": "feature/SPEC-",
      "main_branch": "main",
      "use_gitflow": true
    }
  },
  "pipeline": {
    "available_commands": [
      "/alfred:1-spec",
      "/alfred:2-build",
      "/alfred:3-sync",
      "/alfred:4-debug"
    ],
    "current_stage": "initialized"
  },
  "project": {
    "name": "dt-rag",
    "description": "Dynamic Taxonomy RAG System - Fullstack hybrid architecture",
    "mode": "personal",
    "locale": "ko",
    "created_at": "2025-10-13T10:00:00Z",
    "initialized": true
  },
  "tags": {
    "auto_sync": true,
    "storage_type": "code_scan",
    "categories": [
      "REQ",
      "DESIGN",
      "TASK",
      "TEST",
      "FEATURE",
      "API",
      "UI",
      "DATA"
    ],
    "code_scan_policy": {
      "no_intermediate_cache": true,
      "realtime_validation": true,
      "scan_tools": [
        "rg",
        "grep"
      ],
      "scan_command": "rg '@TAG' -n",
      "philosophy": "TAG의 진실은 코드 자체에만 존재"
    }
  },
  "tech_stack": {
    "projectType": "fullstack",
    "projectLanguage": "hybrid",
    "languages": {
      "backend": "python",
      "frontend": "typescript"
    },
    "frameworks": {
      "backend": "fastapi",
      "frontend": "nextjs"
    },
    "testFramework": {
      "backend": "pytest",
      "frontend": "vitest"
    },
    "linter": {
      "backend": "black",
      "frontend": "eslint"
    }
  },
  "features": [
    "dynamic-taxonomy",
    "hybrid-search",
    "multi-agent-debate",
    "agent-growth-platform",
    "feature-flags"
  ],
  "backup": {
    "enabled": true,
    "retentionDays": 30
  }
}
