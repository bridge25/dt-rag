# DT-RAG Frontend Environment Configuration

# API Configuration
NEXT_PUBLIC_API_BASE_URL=http://localhost:8000
NEXT_PUBLIC_WS_URL=ws://localhost:8000/ws

# Feature Flags
NEXT_PUBLIC_ENABLE_DEV_TOOLS=true
NEXT_PUBLIC_ENABLE_ANALYTICS=false
NEXT_PUBLIC_ENABLE_ERROR_TRACKING=false

# Performance Configuration
NEXT_PUBLIC_VIRTUAL_SCROLL_OVERSCAN=10
NEXT_PUBLIC_SEARCH_DEBOUNCE_MS=300
NEXT_PUBLIC_MAX_SEARCH_RESULTS=1000

# Accessibility Configuration
NEXT_PUBLIC_ENABLE_HIGH_CONTRAST=true
NEXT_PUBLIC_ENABLE_SCREEN_READER=true
NEXT_PUBLIC_ENABLE_KEYBOARD_NAV=true

# Development Configuration (development only)
ANALYZE_BUNDLE=false
DISABLE_ESLINT=false