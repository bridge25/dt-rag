# Environment Configuration for Dynamic Taxonomy RAG

# Database
DB_PASSWORD=dt_rag_secure_password
DATABASE_URL=postgresql://dt_rag_user:dt_rag_secure_password@db:5432/dt_rag

# Redis
REDIS_URL=redis://redis:6379

# API Configuration - GPT Style Defaults
API_KEY_USER=usr_local_dev_key
API_KEY_OPS=ops_local_dev_key
API_KEY_ADMIN=adm_local_dev_key
ENVIRONMENT=development

# Service URLs (for production)
API_BASE_URL=http://localhost:8000
ORCHESTRATION_URL=http://localhost:8001
STAGING_API_BASE=http://localhost:8000

# Frontend Configuration
NEXT_PUBLIC_API_BASE_URL=http://localhost:8000
NEXT_PUBLIC_ORCHESTRATION_URL=http://localhost:8001
NEXT_PUBLIC_MOCK_MODE=false

# Monitoring
PROMETHEUS_ENABLED=true
LOG_LEVEL=INFO

# Security
SECRET_KEY=your-secret-key-for-jwt-here
CORS_ORIGINS=http://localhost:3000,http://localhost:8000

# Performance
MAX_WORKERS=4
CACHE_TTL=300
REQUEST_TIMEOUT=30